{
  "name": "Jot Tab Notes",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Offline first, private not taking in the browser",
  "permissions": ["storage", "scripting"],
  "chrome_url_overrides": {
    "newtab": "./index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
